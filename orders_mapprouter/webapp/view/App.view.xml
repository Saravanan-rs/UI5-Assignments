<mvc:View controllerName="ordersmapprouter.controller.App"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <App>
        <Page id="page" title="{i18n>title}">
            <content />
        </Page>
        <List items="{/Orders}">
            <items>
                <StandardListItem title="{CustomerID}"></StandardListItem>
                
            </items>

        </List>
    </App>
    
</mvc:View>
